"        mutt [-nx] [-e <コマンド>] [-a <ファイル>] [-F <ファイル>] \n"
"             [-H <ファイル>] [-i <ファイル>] [-s <題名>] [-b <アドレス>] \n"
"             [-c <アドレス>] <アドレス> [ ... ]\n"
